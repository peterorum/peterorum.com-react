{
  "name": "peterorum.com",
  "version": "1.0.0",
  "description": "Personal site using react",
  "main": "index.js",
  "scripts": {
    "clean-build": "rimraf dist/*",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --hot",
    "build": "npm run clean-build && cross-env NODE_ENV=production webpack --mode production",
    "test": "cross-env NODE_ENV=test npm run lint && jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ignore-path .gitignore -- .",
    "lint:css": "stylelint './src/**/*.js'"
  },
  "author": "peter@peterorum.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peterorum/peterorum.com-react"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-react-constant-elements": "^7.6.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24.9.0",
    "jest-dom": "^3.5.0",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react-testing-library": "^6.1.2",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "@fullpage/react-fullpage": "^0.1.16",
    "install": "^0.13.0",
    "npm": "^6.13.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sanitize.css": "^8.0.1",
    "styled-components": "^4.3.2"
  }
}
